    "react-navigation": "^1.0.0-beta.27"
  },
  "devDependencies": {
    "@types/expo": "^31.0.1",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.13",
    "@types/react-native": "^0.57.15",
    "@types/react-navigation": "^2.13.7",
    "@types/react-test-renderer": "^16.0.3",
    "babel-cli": "^6.24.1",
    "babel-preset-expo": "^5.0.0",
    "jest-expo": "^31.0.0",
    "prettier": "1.15.3",
